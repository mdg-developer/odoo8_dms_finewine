<openerp>
	<data>
		
		
		<record model="ir.ui.view" id="view_picking_form_inherit_qty">
			<field name="name">stock.move.tree</field>
			<field name="inherit_id" ref="stock.view_move_picking_tree" />
			<field name="model">stock.move</field>
			<field name="arch" type="xml">
				<data>
					<!-- expr="//page/field[@name='invoice_line_ids']/tree/field[@name='name']" -->
					<xpath expr="//field[@name='product_uom_qty']" position="replace">
						<field name="product_uom_qty" on_change="onchange_quantity(product_id, product_uom_qty, product_uom, product_uos)"/>
						<field name="bigger_qty" />
						<field name="big_uom_id" string="Bigger UOM"/>
						<field name="smaller_qty" />
						<field name="smaller_uom_id" string="Smaller UOM"/>
						
					</xpath>
					
				</data>
			</field>
		</record>
		
	</data>
</openerp>