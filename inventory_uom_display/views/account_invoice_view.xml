<openerp>
	<data>
		
		
		<record model="ir.ui.view" id="invoice_form_inherit_uom_qty">
			<field name="name">account.invoice.form</field>
			<field name="inherit_id" ref="account.invoice_form" />
			<field name="model">account.invoice</field>
			<field name="arch" type="xml">
				<data>
					<!-- expr="//page/field[@name='invoice_line_ids']/tree/field[@name='name']" -->
					<xpath expr="//page/field[@name='invoice_line']/tree/field[@name='quantity']" position="before">
						
						<field name="bigger_qty" />
						<field name="big_uom_id" string="Bigger UOM" />
						<field name="smaller_qty" />
						<field name="smaller_uom_id" string="Smaller UOM" />
						
					</xpath>
					
				</data>
			</field>
		</record>
		
	</data>
</openerp>