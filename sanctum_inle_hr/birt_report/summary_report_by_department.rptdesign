            <xml-property name="queryText"><![CDATA[select row_number() over (order by department_id) as serial_number,dept_name,count(emp_name) as emp_no,
        sum(coalesce(basic_salary, 0)) as basic_salary,
	sum(coalesce(net, 0)) as net,
	sum(coalesce(late_deduction, 0)) as late_deduction,
	sum(coalesce(fine_deduction, 0)) as fine_deduction,
	sum(coalesce(leave_deduction, 0)) as leave_deduction,
	sum(coalesce(ssb_fee, 0)) as ssb_fee,
	sum(coalesce(income_tax, 0)) as income_tax,
	sum(coalesce(asbent_leave, 0)) as asbent_leave ,
	sum(coalesce(usd_salary_rule, 0)) as usd_salary_rule
	
from(

        select  dept_name, employeeid, emp_name, job_name,department_id,
        sum(coalesce(child.basic_salary, 0)) as basic_salary,
	sum(coalesce(child.net, 0)) as net,
	sum(coalesce(child.late_deduction, 0)) as late_deduction,
	sum(coalesce(child.fine_deduction, 0)) as fine_deduction,
	sum(coalesce(child.leave_deduction, 0)) as leave_deduction,
	sum(coalesce(child.ssb_fee, 0)) as ssb_fee,
	sum(coalesce(child.income_tax, 0)) as income_tax,
	sum(coalesce(child.asbent_leave, 0)) as asbent_leave,	
	sum(coalesce(child.usd_salary_rule, 0)) as usd_salary_rule

	
	from
	(
		select payslip.*,
		case
			when lower(payslip_line.code) = lower('BASIC')
			then payslip_line.total
		end as basic_salary,

		case
			when lower(payslip_line.code) = lower('NET')
			then payslip_line.total
		end as net,

		case
			when lower(payslip_line.code) = lower('LAT')
			then payslip_line.total
		end as late_deduction,

		case
			when lower(payslip_line.code) = lower('FINE')
			then payslip_line.total
		end as fine_deduction,

		case
			when lower(payslip_line.code) = lower('LVD')
			then payslip_line.total
		end as leave_deduction,

		case
			when lower(payslip_line.code) = lower('SSB')
			then payslip_line.total
		end as ssb_fee,

		case
			when lower(payslip_line.code) = lower('ICT')
			then payslip_line.total
		end as income_tax,

		case
			when lower(payslip_line.code) = lower('ALVD')
			then payslip_line.total
		end as asbent_leave,
		case
			when lower(payslip_line.code) = lower('USD')
			then payslip_line.total
		end as usd_salary_rule

		
		FROM
		(
			select he.name_related as emp_name,he.id as employeeid,hd.id as department_id,
			hp.date_from, hp.date_to,
			hj.name as job_name, hd.name as dept_name, hp.id as id,
			extract(month from hp.date_to) as month,
			extract(year from hp.date_to) as year
			from hr_payslip hp, hr_employee he, hr_job hj, hr_department hd,hr_contract hc
			where hp.employee_id = he.id
			and hp.contract_id=hc.id
			and hc.job_id = hj.id
			and he.department_id = hd.id
			and hp.date_from =  ?
			and hp.date_to =?
		)payslip,
		(
			select round(total) as total,slip_id, code
			from hr_payslip_line HP
		)payslip_line	

		where payslip.id = payslip_line.slip_id
) child
			group by dept_name, employeeid, emp_name, job_name,department_id
			order by dept_name) A
			group by A.dept_name,A.department_id]]></xml-property>
