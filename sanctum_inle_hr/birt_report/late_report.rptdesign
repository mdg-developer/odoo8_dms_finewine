            <xml-property name="queryText"><![CDATA[select row_number() over (order by emp_name) as id,dept_name,emp_name,job_name,(WORKLD+WORKLD1+WORKLD2+WORKLD3+WORKLD4+WORKLD5+WORKLD6+WORKLD7+WORKLD8+WORKLD9
+WORKLD10+WORKLD11+WORKLD12+WORKLD13+WORKLD14+WORKLD15+WORKLD16+WORKLD17+WORKLD18+WORKLD19
+WORKLD20+WORKLD21+WORKLD22+WORKLD23) as count

from
(
	select dept_name, emp_name, job_name,
	sum(coalesce(child.WORKLD, 0)) as WORKLD,
	sum(coalesce(child.WORKLD1, 0)) as WORKLD1,
	sum(coalesce(child.WORKLD2, 0)) as WORKLD2,
	sum(coalesce(child.WORKLD3, 0)) as WORKLD3,
	sum(coalesce(child.WORKLD4, 0)) as WORKLD4,
	sum(coalesce(child.WORKLD5, 0)) as WORKLD5,
	sum(coalesce(child.WORKLD6, 0)) as WORKLD6,
	sum(coalesce(child.WORKLD7, 0)) as WORKLD7,
	sum(coalesce(child.WORKLD8, 0)) as WORKLD8,
	sum(coalesce(child.WORKLD9, 0)) as WORKLD9,
	sum(coalesce(child.WORKLD10, 0)) as WORKLD10,
	sum(coalesce(child.WORKLD11, 0)) as WORKLD11,
	sum(coalesce(child.WORKLD12, 0)) as WORKLD12,
	sum(coalesce(child.WORKLD13, 0)) as WORKLD13,
	sum(coalesce(child.WORKLD14, 0)) as WORKLD14,
	sum(coalesce(child.WORKLD15, 0)) as WORKLD15,
	sum(coalesce(child.WORKLD16, 0)) as WORKLD16,
	sum(coalesce(child.WORKLD17, 0)) as WORKLD17,
	sum(coalesce(child.WORKLD18, 0)) as WORKLD18,
	sum(coalesce(child.WORKLD19, 0)) as WORKLD19,
	sum(coalesce(child.WORKLD20, 0)) as WORKLD20,
	sum(coalesce(child.WORKLD21, 0)) as WORKLD21,
	sum(coalesce(child.WORKLD22, 0)) as WORKLD22,
	sum(coalesce(child.WORKLD23, 0)) as WORKLD23



	
	from
	(
		select payslip.*,

		case
			when lower(payslip_line.code) = lower('WORKLD')
			then payslip_line.number_of_days
		end as WORKLD,

		case
			when lower(payslip_line.code) = lower('WORKLD1')
			then payslip_line.number_of_days
		end as WORKLD1,

		case
			when lower(payslip_line.code) = lower('WORKLD2')
			then payslip_line.number_of_days
		end as WORKLD2,

		case
			when lower(payslip_line.code) = lower('WORKLD3')
			then payslip_line.number_of_days
		end as WORKLD3,

		case
			when lower(payslip_line.code) = lower('WORKLD4')
			then payslip_line.number_of_days
		end as WORKLD4,

		case
			when lower(payslip_line.code) = lower('WORKLD5')
			then payslip_line.number_of_days
		end as WORKLD5,

		case
			when lower(payslip_line.code) = lower('WORKLD6')
			then payslip_line.number_of_days
		end as WORKLD6,

		case
			when lower(payslip_line.code) = lower('WORKLD7')
			then payslip_line.number_of_days
		end as WORKLD7,
		
		case
			when lower(payslip_line.code) = lower('WORKLD8')
			then payslip_line.number_of_days
		end as WORKLD8,
		
		case
			when lower(payslip_line.code) = lower('WORKLD9')
			then payslip_line.number_of_days
		end as WORKLD9,
		case
			when lower(payslip_line.code) = lower('WORKLD10')
			then payslip_line.number_of_days
		end as WORKLD10,

		case
			when lower(payslip_line.code) = lower('WORKLD11')
			then payslip_line.number_of_days
		end as WORKLD11,

		case
			when lower(payslip_line.code) = lower('WORKLD12')
			then payslip_line.number_of_days
		end as WORKLD12,

		case
			when lower(payslip_line.code) = lower('WORKLD13')
			then payslip_line.number_of_days
		end as WORKLD13,

		case
			when lower(payslip_line.code) = lower('WORKLD14')
			then payslip_line.number_of_days
		end as WORKLD14,

		case
			when lower(payslip_line.code) = lower('WORKLD15')
			then payslip_line.number_of_days
		end as WORKLD15,

		case
			when lower(payslip_line.code) = lower('WORKLD16')
			then payslip_line.number_of_days
		end as WORKLD16,

		case
			when lower(payslip_line.code) = lower('WORKLD17')
			then payslip_line.number_of_days
		end as WORKLD17,
		
		case
			when lower(payslip_line.code) = lower('WORKLD18')
			then payslip_line.number_of_days
		end as WORKLD18,
		
		case
			when lower(payslip_line.code) = lower('WORKLD19')
			then payslip_line.number_of_days
		end as WORKLD19,
                case
			when lower(payslip_line.code) = lower('WORKLD20')
			then payslip_line.number_of_days
		end as WORKLD20,

		case
			when lower(payslip_line.code) = lower('WORKLD21')
			then payslip_line.number_of_days
		end as WORKLD21,

		case
			when lower(payslip_line.code) = lower('WORKLD22')
			then payslip_line.number_of_days
		end as WORKLD22,

		case
			when lower(payslip_line.code) = lower('WORKLD23')
			then payslip_line.number_of_days
		end as WORKLD23

			
		FROM
		(
			select he.name_related as emp_name, he.manager as emp_is_manager,
			hp.date_from, hp.date_to,he.id as employee_id,
			hj.name as job_name, hd.name as dept_name, hp.id as id,
			extract(month from hp.date_to) as month,
			extract(year from hp.date_to) as year
			from hr_payslip hp, hr_employee he, hr_job hj, hr_department hd,hr_contract hc
			where hp.employee_id = he.id
			and hp.contract_id=hc.id
			and hc.job_id=hj.id
			and he.department_id = hd.id
			and hp.date_to between ? and ?
		)payslip,
		(
			select code,number_of_days,payslip_id as slip_id from hr_payslip_worked_days

		)payslip_line		
		
		where payslip.id = payslip_line.slip_id

	
	)child

	group by dept_name, emp_name, job_name
	order by dept_name
)parent;

]]></xml-property>
