<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
		<record model="ir.ui.view" id="view_hr_payslip_customize_form">
			<field name="name">hr.payslip.view.form</field>
			<field name="model">hr.payslip</field>
			<field name="inherit_id" ref="hr_payroll.view_hr_payslip_form" />
			<field name="arch" type="xml">
                
				<xpath expr="//tree/field[@name='number_of_hours']" position="after">
					<field name="total_month_day" invisible="1"/>
                    <field name="working_month" invisible="1"/>					
                    			
				</xpath>

			</field>
		</record>                   
    </data>
</openerp>
