            <xml-property name="queryText"><![CDATA[select row_number() over (order by hj.sequence) as id,he.name_related as emp_name ,age(?,date_start) as service_year,hj.name as post,hd.name as dept_name,hc.date_start as permanent_date,hc.joining_date as joining_date 
from hr_contract hc,hr_employee he,hr_job hj,hr_department hd
where he.job_id=hj.id 
and he.department_id=hd.id 
and he.contract_id=hc.id
]]></xml-property>
