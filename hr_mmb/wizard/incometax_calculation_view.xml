<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>


		<record id="view_income_report_form" model="ir.ui.view">
			<field name="name">Report Form</field>
			<field name="model">incometax_cal.report</field>
			<field name="arch" type="xml">
				<form string="Customize Reports" version="7.0">


					<group col="1">
						<b>Reports</b>
						<br />

						<a href="http://localhost:8080/birt/frameset?__report=incometax_calculation_report.rptdesign"
							target="_blank">IncomeTax Calculation for One year</a>

						<br />

					</group>


					<footer>
						<button string="Close" class="oe_highlight" special="cancel" />
