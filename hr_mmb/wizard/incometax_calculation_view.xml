		<record id="action_incometax_cal_report" model="ir.actions.act_window">
			<field name="name">Print Report</field>
			<field name="res_model">incometax_cal.report</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="view_id" ref="view_income_report_form" />
			<field name="target">new</field>
		</record>

		<menuitem action="action_incometax_cal_report" id="menu_incometax_cal_report1"
			name="IncomeTax Calculation for One year" parent="menu_hr_available_incometaxs_report" sequence="2"/>			
	</data>
