            <xml-property name="queryText"><![CDATA[select row_number() over (order by trim(name)) as seq,name,credit_limit,
(select COALESCE(sum(debit-credit),0) 
from account_move am,account_move_line aml,account_account aa
where am.id=aml.move_id
and aml.account_id=aa.id
and type='receivable'
and reconcile_id is null
and aml.partner_id=rp.id
and am.date<?
) opening_balance,
(select COALESCE(sum(amount_total),0) 
from sale_order
where partner_id=rp.id
and ((date_order at time zone 'utc' )at time zone 'asia/rangoon')::date=now()::date
and state not in ('draft','cancel')
and payment_type='credit'
) credit_sales_amt,
(select COALESCE(sum(av.amount),0)
from account_voucher av,account_voucher_line avl,account_move_line aml,sale_order so
where av.id=avl.voucher_id
and avl.move_line_id=aml.id
and aml.ref=so.name
and av.partner_id=rp.id
and av.date=now()::date
and av.state not in ('draft','cancel')
and av.type='receipt'
and payment_type='credit'
) credit_collection_amount,
(select COALESCE((sum(amount_untaxed)+sum(total_dis))-sum(amount_total)-sum(amount_tax),0)
from sale_order
where partner_id=rp.id
and ((date_order at time zone 'utc' )at time zone 'asia/rangoon')::date=now()::date
and state not in ('draft','cancel')
and payment_type='credit'
) discount,0 sales_return,0 transport_charges
from res_partner rp,sale_team_customer_rel rel
where rp.id=rel.partner_id
and customer='true'
--$company_id$
--$branch_id$
--$sales_team_id$]]></xml-property>
