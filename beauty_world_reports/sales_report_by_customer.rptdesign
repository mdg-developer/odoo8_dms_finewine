            <xml-property name="queryText"><![CDATA[select row_number() over (order by month_number,customer_code),*
from 
(select rp.customer_code,rp.name customer,sum(amount_total) amount_total,
date_part('year',(date_order at time zone 'utc' )at time zone 'asia/rangoon')::int date_order_year,
initcap(to_char(to_timestamp (date_part('month',(date_order at time zone 'utc' )at time zone 'asia/rangoon')::text, 'MM'), 'TMmonth')) date_order_month,
date_part('month',(date_order at time zone 'utc' )at time zone 'asia/rangoon') as month_number
from sale_order so,res_partner rp
where so.partner_id=rp.id
and so.state not in ('draft','cancel')
and ((date_order at time zone 'utc' )at time zone 'asia/rangoon')::date between (select date_start from account_period where id=?) 
and (select date_stop from account_period where id=?)
--$month$
--$company_id$
--$branch_id$
--$sales_team_id$
--$customer_id$
group by partner_id,rp.customer_code,rp.name,date_part('year',(date_order at time zone 'utc' )at time zone 'asia/rangoon')::int,
initcap(to_char(to_timestamp (date_part('month',(date_order at time zone 'utc' )at time zone 'asia/rangoon')::text, 'MM'), 'TMmonth')),
date_part('month',(date_order at time zone 'utc' )at time zone 'asia/rangoon')
)A]]></xml-property>
