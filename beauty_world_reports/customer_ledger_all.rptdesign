            <xml-property name="queryText"><![CDATA[select row_number() over (order by trim(rp.name)) as seq,rp.name,rp.customer_code,credit_limit,
(
	select COALESCE(sum(aml.debit)- sum(aml.credit),0) as opening
	from account_move_line aml,account_account aa,account_move am
	left join account_invoice ai on am.id=ai.move_id
	where am.id=aml.move_id
	and aml.account_id=aa.id
	and aa.type='receivable'
	--and reconcile_id is null
	and rp.id=aml.partner_id
	and am.date <= ?
	and (am.name not like '%INV%' or ai.payment_type ='credit')
	--$amlanalytic$
) opening_balance,
(
	select  COALESCE(sum(credit_sale),0) credit from (
	select (ai.amount_untaxed + ai.discount_total) as credit_sale
	from account_invoice ai,account_invoice_line ail
	where rp.id=ai.partner_id
	and ai.id=ail.invoice_id
	and ((ai.date_invoice at time zone 'utc' )at time zone 'asia/rangoon')::date between ? and ?
	and ai.state not in ('draft','cancel')	
	and ai.type ='out_invoice'
	and ai.payment_type='credit'
	--$ailanalytic$
	group by ai.amount_untaxed,ai.discount_total,ai.id)credit
) credit_sales_amt,
(
	select COALESCE(av.amount,0)
	from account_voucher av,account_voucher_line avl,account_move_line aml,sale_order so
	where av.id=avl.voucher_id
	and avl.move_line_id=aml.id
	and aml.ref=so.name
	and rp.id=av.partner_id
	and av.date between ? and ?
	and av.state not in ('draft','cancel')
	and av.type='receipt'
	and so.payment_type='credit'
	--$amlanalytic$
	group by av.id
) credit_collection_amount,
(
	select COALESCE(sum(discount_total),0) discount_total from (
	select ai.discount_total as discount_total
	from account_invoice ai,account_invoice_line ail
	where rp.id=ai.partner_id
	and ((ai.date_invoice at time zone 'utc' )at time zone 'asia/rangoon')::date between ? and ?
	and ai.state not in ('draft','cancel')	
	and ai.type ='out_invoice'
	and ai.payment_type='credit'
	group by ai.id ) discount
	--$ailanalytic$
) discount,
-- (
-- 	select COALESCE(sum(debit-credit),0) 
-- 	from account_move am,account_move_line aml,account_account aa
-- 	where am.id=aml.move_id
-- 	and aml.account_id=aa.id
-- 	and aa.code='701000'
-- 	and rp.id=aml.partner_id
-- 	and am.date between ? and ?
-- 	--$amlanalytic$
-- ) transport_charges,
(
	select sum(return_amt) as return_amt from (
	select COALESCE(ai.amount_total,0) as return_amt from 
	account_invoice ai,account_invoice_line ail,account_journal aj
	where ai.id=ail.invoice_id and ai.journal_id=aj.id 
	and rp.id=ai.partner_id
	and aj.name='Sale Return Journal' 
	and ai.state not in ('draft','cancel')
	and ai.type='out_refund'
	and ai.payment_type='credit'
	and date_invoice::date between ? and ?
	group by ai.id )return
	--$ailanalytic$
) sales_return,
( 
	select  COALESCE(sum(debit-credit),0) as closing
	from account_move_line aml,account_account aa,account_move am
	left join account_invoice ai on am.id=ai.move_id
	where am.id=aml.move_id
	and aml.account_id=aa.id
	and aa.type='receivable'
	--and aml.reconcile_id is null
	and aml.partner_id=rp.id
	and am.date <= ?
	and (am.name not like '%INV%' or ai.payment_type ='credit')
	and rp.id=46121
	--$amlanalytic$
) closing_balance
from res_partner rp,sale_team_customer_rel rel
where rp.id=rel.partner_id
and rp.customer='true'
and rp.active='true'
--$company_id$
--$branch_id$
--$sales_team_id$
--$customer_id$
group by rp.id
order by name]]></xml-property>
