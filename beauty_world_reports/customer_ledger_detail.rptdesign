            <xml-property name="queryText"><![CDATA[select rc.id company_id,rc.name company_name,rb.id branch_id,
rb.branch_code,ccs.id sale_team_id,ccs.code sale_team_code,
rp.id customer_id,rp.name customer_name
from res_company rc
left join res_branch rb on rc.id=rb.res_company_id
left join crm_case_section ccs on ccs.branch_id=rb.id 
left join sale_team_customer_rel rel on rel.sale_team_id=ccs.id 
left join res_partner rp on rp.id=rel.partner_id
where rp.customer=true
and rp.active=true
and ccs.active=true
order by rp.name]]></xml-property>
