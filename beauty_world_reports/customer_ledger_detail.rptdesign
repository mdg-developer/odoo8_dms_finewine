            <xml-property name="queryText"><![CDATA[with receivable_amount as (
	select 'INV'::character varying as type,trim(rp.name) customer_name,rp.id customer_id,'Opening Balance'::character varying ref_no,null::date date,null::numeric inv,null::numeric collection,0 cn,0 discount,COALESCE(sum(debit-credit),0) closing_balance,1 as sequence
	from account_move am
	left join account_move_line aml on (am.id=aml.move_id)
	left join account_account aa on (aml.account_id=aa.id)
	left join res_partner rp on (aml.partner_id=rp.id)
	where aa.type='receivable'
	and reconcile_id is null	
	and am.date<?
	--$amlanalytic$
	group by rp.name,customer_id)
select null::character varying as type,trim(rp.name) customer_name,rp.id customer_id,'Opening Balance'::character varying ref_no,null::date date,null::numeric inv,null::numeric collection,
null::numeric cn,null::numeric discount,
case when (select closing_balance from receivable_amount where customer_id=rp.id) is null then 0 else (select closing_balance from receivable_amount where customer_id=rp.id) end as closing_balance,1 as sequence
from res_partner rp
left join account_move_line aml on (rp.id=aml.partner_id)
join account_move am on (am.id=aml.move_id)
join account_account aa on (aml.account_id=aa.id)
join account_invoice inv on (rp.id=inv.partner_id)
where rp.active='true'
and rp.customer='true'
and payment_type='credit'
--$company_id$
--$branch_id$
--$sales_team_id$
--$customer_id$
--$amlanalytic$
group by rp.name,rp.id

union

select type,customer_name,customer_id,ref_no,date,inv,COALESCE(collection,0)collection,cn,discount,(inv-collection-cn) closing_balance,sequence 
from 
(
select 'INV'::character varying as type,trim(rp.name) customer_name,number ref_no,date_invoice date,sum(amount_total) inv,
(select COALESCE(sum(avl.amount),0) 
from account_voucher av,account_voucher_line avl,account_move_line aml,account_invoice ai
where av.id=avl.voucher_id
and avl.move_line_id=aml.id
and aml.ref=ai.origin
and ai.number=inv.number
and av.state not in ('draft','cancel')
and av.type='receipt'
--$amlanalytic$
and payment_type='credit') collection,
(select COALESCE(sum(ai.amount_total),0)return_amt from 
account_invoice ai,account_journal aj, account_invoice_line ail 
where ai.journal_id=aj.id and ai.id=ail.invoice_id
and aj.name='Sale Return Journal' 
and ai.type='out_refund'
and ai.number=inv.number
and ai.state not in ('draft','cancel')
and date_invoice between ? and ?
--$ailanalytic$
)cn,
(select COALESCE((sum(amount_untaxed)+sum(discount_total))-sum(amount_total)-sum(amount_tax),0)
from account_invoice, account_invoice_line ail
where number=inv.number and account_invoice.id=ail.invoice_id
and state not in ('draft','cancel')
--$ailanalytic$
and payment_type='credit') discount,2 as sequence,rp.id customer_id
from account_invoice inv,res_partner rp, account_invoice_line ail
where inv.partner_id=rp.id and ail.invoice_id=inv.id
and state not in ('draft','cancel')
and date_invoice between ? and ?
and payment_type='credit'
--$company_id$
--$branch_id$
--$sales_team_id$
--$customer_id$
--$ailanalytic$
group by rp.name,rp.id,number,date_invoice
)A
order by sequence]]></xml-property>
