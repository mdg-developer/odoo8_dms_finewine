<openerp>
	<data>


		<record model="ir.ui.view" id="view_picking_form_inherit_qty">
			<field name="name">stock.move.tree</field>
			<field name="inherit_id" ref="stock.view_move_picking_tree" />
			<field name="model">stock.move</field>
			<field name="arch" type="xml">
				<data>
					<xpath expr="//field[@name='product_uom_qty']" position="after">
						<field name="gross_weight" invisible="1" />
						<field name="net_weight" invisible="1" />
						<field name="smaller_qty" invisible="1"/>
						<field name="bigger_qty" readonly="1"/>
					</xpath>

				</data>
			</field>
		</record>

		<record id="view_stock_enter_transfer_details_inherit" model="ir.ui.view">
			<field name="name">stock.transfer_details</field>
			<field name="model">stock.transfer_details</field>
			<field name="type">tree</field>
			<field name="inherit_id" ref="stock.view_stock_enter_transfer_details" />
			<field name="arch" type="xml">
				<xpath expr="//field[@name='item_ids']/tree/field[@name='quantity']"
					position="after">
					<field name="bigger_qty" readonly="1"/>
				</xpath>

			</field>
		</record>
	</data>
</openerp>