            <xml-property name="queryText"><![CDATA[select  AA.name , sum(AA.debit) as debit, sum(AA.credit) as credit from (
select AAA.partner_id , RP.name,
case when AAL.amount>0 then
	sum(AAL.amount)
	end as debit,
	
case when AAL.amount<0 then
	sum(-AAL.amount)
	end as credit
 from 
account_analytic_account AAA , account_analytic_line AAL ,account_account AA,res_partner RP
where AAA.id = AAL.account_id
and AAL.general_account_id = AA.id
and RP.id= AAA.partner_id
and AAA.name= ?
group by AAA.partner_id ,RP.name ,AAL.amount
)AA 
group by AA.partner_id , AA.name]]></xml-property>
