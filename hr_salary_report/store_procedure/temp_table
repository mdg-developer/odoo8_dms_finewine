  emp_id integer,
  emp_name character varying,
  basic_salary numeric,
  allowance numeric,
  contribution numeric,
  cla numeric,
  total numeric,
  ssb numeric,
  total_value numeric,
  income_tax numeric,
  causlv numeric,
  annuallv numeric,
  unplv1 numeric,
  unplv2 numeric,
  medlv1 numeric,
  medlv2 numeric,
  medlv3 numeric,
  matlv numeric,
  abslv1 numeric,
  abslv2 numeric,
  workld1 numeric,
  workld2 numeric,
  workld3 numeric,
  workld4 numeric,
  workld5 numeric
)
WITH (
  OIDS=FALSE
