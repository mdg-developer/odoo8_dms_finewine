<?xml version="1.0" ?>
<openerp>
	<data>
    <record id="customer_target_view" model="ir.ui.view">
      <field name="name">customer.target.form</field>
      <field name="model">customer.target</field>
      <field name="priority">1</field>
      <field name="arch" type="xml">
        <form string="Customer Target" version="7.0">
          <group>  
          <group >
            
            <field name="partner_id"/>
            <field name="outlet_type"/>
            <field name="township"/>
            <field name="address"/>
            <field name="date"/>
          </group>
          <group >
            
            <field name="branch_id"/>
            <field name="section_ids" widget="many2many_tags"/>
            <field name="delivery_team_id"/>
            <field name="delivery"/>
          </group>
          <group >
            
            <field name="updated_by"/>
            <field name="updated_time"/>
           
          </group>
          </group>
          <notebook>
			<page string="Product">
				<field name="target_line_ids" >
					
					<tree string="Product" editable="bottom">
						<field name="product_id"/>
						<field name="product_uom"/>										
						<field name="price"/>												
						<field name="month1"/>
						<field name="month2"/>
						<field name="month3"/>	
						<field name="month4"/>
						<field name="month5"/>
						<field name="month6"/>
						<field name="6ams"/>
						<field name="target_qty"/>
						<field name="ach_qty"/>
					</tree>
				</field>
			</page>
		</notebook>
          <footer>
             <button name="update" string="Update" type="object" class="oe_highlight"/>
            or
            <button string="Close" class="oe_link" special="cancel"/>
           
          </footer>
        </form>
      </field>
    </record>

   
    
  </data> 
</openerp>
