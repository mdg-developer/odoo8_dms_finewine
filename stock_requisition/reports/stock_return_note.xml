            <div class="row mt32 mb32">
                <div class="col-xs-2" t-if="o.name">
                    <strong>(SRN)Ref;No:</strong>
                    <p t-field="o.name"/>
                </div>
                <div class="col-xs-2" t-if="o.note_id">
                    <strong>(GIN)Ref;No. :</strong>
                    <p t-field="o.note_id"/>
                </div>
                <div class="col-xs-2" t-if="o.sale_team_id">
                    <strong>Sales Team:</strong>
                    <p t-field="o.sale_team_id"/>
                </div>
                <div class="col-xs-2" t-if="o.so_no">
                    <strong>Sales Order/Inv Ref;No. :</strong>
                    <p t-field="o.so_no"/>
                </div>
               <!--  <div class="col-xs-2" t-if="o.return_from">
                    <strong>Return From :</strong>
                    <p t-field="o.return_from"/>
                </div> -->
            </div>
			
			<div class="row mt32 mb32">
                <div class="col-xs-2" t-if="o.name">
                    <strong>Return From :</strong>
                    <p t-field="o.return_from"/>
                </div>
                <div class="col-xs-2" t-if="o.vehicle_id">
                    <strong>Vehicle No :</strong>
                    <p t-field="o.vehicle_id.name"/>
                </div>
                <div class="col-xs-2" t-if="o.return_date">
                    <strong>Date of Return:</strong>
                    <p t-field="o.return_date"/>
                </div>
                <div class="col-xs-2" t-if="o.company_id">
                    <strong>Company. :</strong>
                    <p t-field="o.company_id.name"/>
                </div>               
            </div>
            
            <table class="table table-condensed">
                <thead>
                    <tr>                    
                        <th>Product</th>                        
                        <th class="text-right">Received Qty</th>
                        <th class="text-right" >Returned Qty</th>
                        <th class="text-right">Sales Qty</th>
                        <th class="text-right">FOC Qty</th>
                        <th>UoM</th>
                        <th class="text-right">Rec Qty</th>
                        <th >Rec Bigger UoM</th>
                        <th class="text-right">Rec Qty</th>
                        <th >Rec Smaller UoM</th>
                        <th >Expiry</th>
                        <th >Remark</th>
                    </tr>
                </thead>
                <tbody class="invoice_tbody">
                    <tr t-foreach="o.p_line" t-as="l">
                        <td><span t-field="l.product_id.name"/></td>
                        <td class="text-right">
                            <span t-field="l.receive_quantity"/>                            
                        </td>
                        <td class="text-right">
                            <span t-field="l.return_quantity"/>
                        </td>
                        <td class="text-right">
                            <span t-field="l.sale_quantity"/>
                        </td>
                        <td class="text-right">
                            <span t-field="l.foc_quantity"/>
                        </td>
                        <td><span t-field="l.product_uom.name"/></td>
                                                
                        <td class="text-right">
                            <span t-field="l.rec_big_quantity"/>
                        </td>
                        
                        <td>
                            <span t-field="l.rec_big_uom_id.name"/>
                        </td>
                        
                         <td class="text-right">
                            <span t-field="l.rec_small_quantity"/>
                        </td>
                        
                        <td><span t-field="l.rec_small_uom_id.name"/></td>
                        
                        <td><span t-field="l.expiry_date"/></td>
                        
                        <td><span t-field="l.remark"/></td>
                    </tr>
                </tbody>
            </table>

