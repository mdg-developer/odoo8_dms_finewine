<template id="stock_return_note">
    <t t-call="report.html_container">
        <t t-foreach="doc_ids" t-as="doc_id">
            <t t-raw="translate_doc(doc_id, doc_model, 'partner_id.lang', 'stock_requisition.stock_return_note_document')"/>
            
