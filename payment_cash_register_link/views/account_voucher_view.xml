<openerp>
  <data>   
    
    <record id="view_vendor_receipt_form_inherit_cashbook" model="ir.ui.view">
			<field name="name">account.voucher.form.inherit</field>
			<field name="model">account.voucher</field>
			<field name="inherit_id" ref="account_voucher.view_vendor_receipt_form" />
			<field name="arch" type="xml">				
				<field name="journal_id" position="after">
				    <field name="bank_statement_id" domain="[('journal_id', '=', journal_id),('date', '=', date),('brand_id', '=', branch_id),('state','=','open')]"/>
				    
				</field>
			</field>
	</record> 
	
    <record id="view_vendor_receipt_dialog_form_inherit_cashbook" model="ir.ui.view">
			<field name="name">account.voucher.form.inherit</field>
			<field name="model">account.voucher</field>
			<field name="inherit_id" ref="account_voucher.view_vendor_receipt_dialog_form" />
			<field name="arch" type="xml">
			    <!-- <field name="currency_id" position="after"/>
			    	
			    	<field name="branch_id" invisible="1"/>
			    </field> -->	
			    			
				<field name="journal_id" position="after">				    
				    <field name="branch_id" />
				    <field name="bank_statement_id" domain="[('journal_id', '=', journal_id),('date', '=', date),('brand_id', '=', branch_id),('state','=','open')]"/>
				</field>
			</field>
	</record>               
	
    <record id="view_vendor_payment_form_inherit_cashbook" model="ir.ui.view">
			<field name="name">account.voucher.payment.form</field>
			<field name="model">account.voucher</field>
			<field name="inherit_id" ref="account_voucher.view_vendor_payment_form" />
			<field name="arch" type="xml">
				<data>
					<field name="journal_id" position="after">
					    <field name="branch_id" />
						<field name="bank_statement_id" domain="[('journal_id', '=', journal_id),('date', '=', date),('brand_id', '=', branch_id),('state','=','open')]"/>
					</field>
					
					<!-- <field name="reconcile" position="after">
						<field name="total_discount" sum="Total Discount" on_change="onchange_discount_amount(total_discount, context)"/>
					</field> -->
				</data>
			</field>
		</record>
  </data>
</openerp>
