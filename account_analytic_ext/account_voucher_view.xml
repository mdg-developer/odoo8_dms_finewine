<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>


		<record model="ir.ui.view" id="inherit_view_vendor_receipt_form">
			<field name="name">account.voucher.customer.form</field>
			<field name="model">account.voucher</field>
			<field name="inherit_id" ref="account_voucher.view_vendor_receipt_form" />
			<field name="arch" type="xml">
				<field name="date_original" position="before">
					<field name="account_analytic_id" />
				</field>
			
				
				<xpath
					expr="//form/sheet/notebook/page/field/tree[@string='Credits']/field[@name='date_original']"
					position="before">
					<field name="account_analytic_id" />
					
				</xpath>					
				<field name="debit" position="before">
					<field name="analytic_account_id" />
				</field>
			</field>
		</record>


		<record model="ir.ui.view" id="inherit_view_vendor_payment_form">
			<field name="name">account.voucher.customer.form</field>
			<field name="model">account.voucher</field>
			<field name="inherit_id" ref="account_voucher.view_vendor_payment_form" />
			<field name="arch" type="xml">
				<field name="date_original" position="before">
					<field name="account_analytic_id" />
				</field>
				<field name="debit" position="before">
					<field name="analytic_account_id" />
				</field>				
			</field>
		</record>		
		
	</data>
	
</openerp>