            <xml-property name="queryText"><![CDATA[select b.*,a.*,(select name from crm_case_section where id = a.section_id) as sale_team,(select name from res_branch where id = a.branch_id) as branch from 
(select ai.section_id,ai.branch_id,count(DISTINCT ai.partner_id) as outlet_count,count(ai.id) as  invoice_count ,ail.promotion_id,
case when ail.uos_id=pt.uom_id and sum(ail.price_unit)=0 then sum(round(quantity,0)) when ail.uos_id=pt.uom_id and sum(ail.price_unit)<0  then 1 when ail.uos_id!=pt.uom_id then sum(round(quantity*(select floor(round(1/factor,2)) from product_uom where id=ail.uos_id),0)) end as quantity,
case when sum(ail.price_unit)=0 then string_agg(DISTINCT  pp.name_template,', ') else  '' end as foc_product,
case when sum(ail.price_unit)<0 then string_agg(DISTINCT  pp.name_template,', ') else  '' end as discount_product,
case when sum(ail.price_unit)<0 then -1 * sum(ail.price_unit)  else  0 end as discount_price
from account_invoice ai,account_invoice_line ail,product_template pt,product_product pp
where ai.id=ail.invoice_id
and pt.id=pp.product_tmpl_id
and pp.id=ail.product_id
and ai.state!='cancel'
and ai.date_invoice between ? and ?
and ail.promotion_id is not null
--$branch_id$
--$sale_team_id$
group by ail.uos_id,pt.uom_id,ail.promotion_id ,ai.section_id,ai.branch_id
)a
left join 
(select promo.id as promo_id,promo.name,promo.code,promo.description,pm.id as main_group_id,pm.name as main_group,(select string_agg(cat.name,', ') from res_partner_category cat where cat.id in (select category_id from promotion_rule_category_rel rel where promotion_id=promo.id)) as customer_tag,
(select string_agg(cha.name,', ') from sale_channel cha where cha.id in (select sale_channel_id from promo_sale_channel_rel rel where promo_id=promo.id)) as channels,
(select string_agg(branch.name,', ') from res_branch branch where branch.id in (select res_branch_id from promos_rules_res_branch_rel rel where promos_rules_id=promo.id)) as branch_ids,
promo.from_date,promo.to_date,
(select string_agg(product.name_template,', ') from product_product product where product.id in (select product_id from promos_rules_product_rel rel where promos_rules_id=promo.id)) as products,
(select string_agg(outlettype.name,', ') from outlettype_outlettype outlettype where outlettype.id in (select outlettype_id from promos_rules_outlettype_rel rel where promos_rules_id=promo.id)) as outlettypes
from promos_rules promo ,product_maingroup pm where 
pm.id=promo.main_group
and promo.active=True)b
on b.promo_id=a.promotion_id
--$principle$

]]></xml-property>
