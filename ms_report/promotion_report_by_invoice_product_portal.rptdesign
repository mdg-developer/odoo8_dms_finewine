            <xml-property name="queryText"><![CDATA[select avl.id,av.comment,date_invoice,acc.code as account_name,acc.name as account_desc,
CASE
    WHEN av.payment_type::text = 'credit'::text THEN 'Credit'::text
    ELSE 'Cash'::text
END AS payment_type
,rp.customer_code as customer_id
,rp.name as customer,av.number as invoice_no ,cc.name as sale_team,pp.name_template as product_desc,pt.description_sale,
avl.quantity as qty,uom.name as uom,avl.price_unit as unit_price,avl.price_subtotal as net_total,avl.discount,
avl.discount_amt,rule.description  as promotion,
CASE
  WHEN avl.price_subtotal < 0 THEN 0
  ELSE avl.price_subtotal + avl.discount_amt END as sub_total,
CASE
  WHEN avl.foc = true THEN 'Y'::text
  ELSE 'N'::text
END AS foc,
CASE
    WHEN rule.id is not null THEN (select name from product_maingroup where id  = rule.main_group)
    ELSE (select name from product_maingroup where id  = pt.main_group)
END AS main_group,
 (SELECT  (replace(replace(array_agg(DISTINCT pp.name_template)::text, '{'::text, ''::text), '}'::text, ''::text))AS array_agg
                  FROM promos_rules_product_rel ,product_product pp 
                  WHERE promos_rules_product_rel.promos_rules_id = avl.promotion_id
                  AND pp.id=promos_rules_product_rel.product_id )
                   AS promotion_product
from account_invoice av ,account_invoice_line avl left join promos_rules rule on avl.promotion_id = rule.id
,account_account acc ,res_partner rp,crm_case_section cc,product_product pp,product_template pt,product_uom uom
where av.id =avl.invoice_id
and acc.id =avl.account_id
and av.state not  in ('draft','cancel')
and rp.id =av.partner_id
and cc.id =av.section_id
and pp.id=avl.product_id
and pt.id=pp.product_tmpl_id
and avl.uos_id = uom.id
and av.date_invoice between ? and ?
--$sale_team_id$
--$branch_id$
--$product_id$
ORDER BY av.date_invoice,cc.name,rp.name ,pp.sequence
]]></xml-property>
