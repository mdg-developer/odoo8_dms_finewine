<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record id="product_receive_sequence" model="ir.sequence.type">
			<field name="name">Stock Receive Sequence</field>
			<field name="code">receive.code</field>
		</record>

		<record id="product_receive_seq" model="ir.sequence">
			<field name="name">Stock Receive Sequence</field>
			<field name="code">receive.code</field>
			<field name="prefix">GRN/%(year)s/</field>
			<field name="padding">7</field>
		</record>

		<record id="pallet_transfer_sequence" model="ir.sequence.type">
			<field name="name">Pallet Transfer Sequence</field>
			<field name="code">transfer.code</field>
		</record>

		<record id="pallet_transfer_seq" model="ir.sequence">
			<field name="name">Pallet Transfer Sequence</field>
			<field name="code">transfer.code</field>
			<field name="prefix">PT/%(year)s/</field>
			<field name="padding">7</field>
		</record>
	</data>
</openerp>