<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="purchase_order_form_inherit_other_information_import" 	model="ir.ui.view">
			<field name="name">purchase.order.form.import</field>
			<field name="model">purchase.order</field>
			<field name="inherit_id" ref="purchase.purchase_order_form" />
			<field name="arch" type="xml">
				<xpath expr="//notebook/page/field[@name='order_line']"
					position="before">
					<button name="import_lines" states="draft" string="Import Purchase Lines"
						icon="gtk-execute" type="object" groups="account.group_account_manager" />
				</xpath>
			</field>
		</record>
	</data>
</openerp>
