<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>

		<record id="branch_product_req_sequence" model="ir.sequence.type">
			<field name="name">Branch Stock Request Sequence</field>
			<field name="code">branch.request.code</field>
		</record>

		<record id="branch_product_req_seq" model="ir.sequence">
			<field name="name">Branch Stock Request Sequence</field>
			<field name="code">branch.request.code</field>
			<field name="prefix">BRFI/%(year)s/</field>
			<field name="padding">7</field>
		</record>

		<record id="branch_gin_req_sequence" model="ir.sequence.type">
			<field name="name">Branch Good Issue Sequence</field>
			<field name="code">branch.gin.code</field>
		</record>

		<record id="branch_gin_req_seq" model="ir.sequence">
			<field name="name">Branch Good Issue Sequence</field>
			<field name="code">branch.gin.code</field>
			<field name="prefix">BGIN/%(year)s/</field>
			<field name="padding">7</field>
		</record>
		
		<record id="branch_grn_req_sequence" model="ir.sequence.type">
			<field name="name">Branch Good Receive Sequence</field>
			<field name="code">branch.grn.code</field>
		</record>

		<record id="branch_grn_req_seq" model="ir.sequence">
			<field name="name">Branch Good Receive Sequence</field>
			<field name="code">branch.grn.code</field>
			<field name="prefix">BGRN/%(year)s/</field>
			<field name="padding">7</field>
		</record>		
	</data>
</openerp>